{
  "compilerOptions": {
      "target": "ES2023",
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "outDir": "./dist",
      "declaration": true,
      "strict": true,
      "sourceMap": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "removeComments": false,
  },
  "include": ["src" ,"index.d.ts"],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
}