{
  "name": "conhos-vscode",
  "version": "1.0.2",
  "displayName": "Conhos",
  "publisher": "kolserdav",
  "icon": "resources/logo.png",
  "description": "Highlights the syntax of the conhos.yml file",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "vscode": "^1.96.0"
  },
  "keywords": [
    "docker",
    "compose",
    "container",
    "containers",
    "cloud",
    "hosting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kolserdav/conhos-vscode"
  },
  "files": [
    "dist",
    "dist-web",
    "index.d.ts"
  ],
  "scripts": {
    "dev": "webpack --mode development --watch",
    "dev:web": "webpack -c webpack.config.web.js --mode development --watch",
    "prebuild": "rimraf dist/* && webpack --mode production && npm run build:web",
    "build:web": "rimraf dist-web/* && webpack -c webpack.config.web.js --mode production",
    "scripts": "node ./dist/scripts.js",
    "package": "vsce package -o out",
    "deploy": "vsce publish ${version} --pat $(cat .azure-dev-api-key)",
    "build": "rimraf out/* && npm run package"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "Conhos Configuration Checker"
    },
    "languages": [
      {
        "id": "yaml",
        "aliases": [
          "YAML",
          "yml"
        ],
        "extensions": [
          "conhos.yml",
          "conhos.yaml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "yaml",
        "path": "./syntaxes/yml.tmLanguage.json",
        "scopeName": "source.yaml"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@types/node": "^22.5.4",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "babel-loader": "^9.2.1",
    "commander": "^12.1.0",
    "eslint": "^9.17.0",
    "eslint-plugin-prettier": "^5.2.1",
    "ts-loader": "^9.5.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "cache-changed": "^1.3.2",
    "rimraf": "^6.0.1",
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12",
    "yaml": "^2.5.1"
  },
  "fhead": {
    "root": "./",
    "repository": "Conhos vscode",
    "patterns": [
      ".ts",
      ".js"
    ],
    "exclude": [
      "node_modules",
      "dist",
      "dist-web"
    ],
    "name": "Sergey Kolmiller",
    "email": "kolserdav@conhos.ru",
    "license": "MIT",
    "licenseText": "See LICENSE file",
    "copyright": "kolserdav, All rights reserved (c)",
    "renewAll": false
  }
}
